		$('#back_login').click(function() {
			$('#send_email').fadeOut('medium', function () {
				$('#login_change').html('Member Login');
				$('#member_login').fadeIn('medium');

			});
		});

		$('.rem').click(function() {
		 if (confirm('Are you sure you want to delete this list?'))
			return true;
		 else
			return false;
		});
		var a;
		$('.bt_login').hover(
		function() {
		$(this).css('color','#005');
		a = $(this).val();
		$(this).attr('value',' ' + $(this).val());
		},
		function() {
		$(this).css('color','#000');
		$(this).attr('value',a);
		}
		);

		$('.bt_register').hover(
		function() {
		$(this).css('color','#000');
		},
		function() {
		$(this).css('color','#fff');
		}

		);

		$('#max').focus(
		function () {
		$('#showonfocus').show();
		}
		);

		$('#max').blur(
		function () {
		if($('#max').val() == "")
			$('#showonfocus').hide();
		}
		);

		$('#wordliststorecheck').click(
			function() {
				return content_show == 'Home';
			});

});

